{
    "manifest_version": 3,
    "name": "Policy Analyzer",
    "version": "1.0",
    "description": "An extension to analyze privacy policies.",
    "action": {
      "default_popup": "index.html",
      "default_icon": "logo.png"
    },
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "host_permissions": [
      "http://*/*",
      "https://*/*"
    ]
  }
  